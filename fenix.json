{
    "$schema": "https://raw.githubusercontent.com/FenixTemplates/FenixTemplates/master/fenix-schema.json",
    "author": "ACharLuk",
    "repoName": "Fenix Default Templates",
    "repoUrl": "https://raw.githubusercontent.com/FenixTemplates/Default/master/",
    "version": 1.0,
    "templates": [
        {
            "id": "fenix-new-template",
            "displayName": "Fenix Template",
            "description": "Template used to create new Fenix templates",
            "language": "Other",
            "category": [
                "Fenix"
            ],
            "img": "https://raw.githubusercontent.com/acharluk/Fenix/master/src/assets/logo.png",
            "files": {
                "download": [
                    {
                        "from": "blank-template.json",
                        "to": "fenix.json"
                    }
                ],
                "open": [
                    "fenix.json"
                ]
            },
            "environment": [
                {
                    "id": "author",
                    "title": "Author",
                    "description": "Enter your name here",
                    "default": "Me"
                }
            ]
        },
        {
            "id": "fenix-example-template",
            "displayName": "Example Template",
            "description": "Example template to test Fenix",
            "language": "Other",
            "category": [
                "Fenix",
                "Testing"
            ],
            "img": "https://raw.githubusercontent.com/acharluk/Fenix/master/src/assets/logo.png",
            "files": {
                "download": [
                    {
                        "from": "test.md",
                        "to": "test.md"
                    }
                ],
                "open": [
                    "test.md"
                ]
            },
            "environment": [
                {
                    "id": "author",
                    "title": "Author",
                    "description": "Enter your name here",
                    "default": "Test author"
                }
            ],
            "command": "echo Hola mundo!"
        }
    ]
}